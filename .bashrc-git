alias cls="clear"
alias v="vim"
alias cp='cp -i'
alias rm='rm -i'
alias chx="chmod +x"
alias sb="source ~/.bashrc"
alias vg="vim ~/.bashrc-git"
alias vz="vim ~/.zshrc"
alias vv="vim ~/.vimrc"
alias cdw="cd ~/git/touch-skin"
alias cdh="cd ~/git/home"
alias f="find . -iname"
alias h="history"
alias s="screen"
alias condb="mysql --default-character-set=utf8 -uroot -hmysql.lo.mixi.jp"
alias prove="/usr/local/mixi-devtools/mixi-test/bin/mixi-prove"
alias mc="/home/bkapps/git/touch-skin/script/devel/service_activation_per_user"
alias agl="ag '--pager=less -R'"
