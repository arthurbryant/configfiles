Namespace('jp.mixi.practice.pc.show')
.define(function (ns){
    ns.provide({
        show: function() {
            $j(".clickMe").on("click", function(){
                $j(".findMe").fadeIn();
            });
            $j(".findMe").on("click", function(){
                var choose = confirm("yes or no");
                if(choose) {
                    $j(".clickMe").fadeOut();
                }
                else {
                    $j(".findMe").fadeOut();
                }
            });
            }
        }
        );
});

