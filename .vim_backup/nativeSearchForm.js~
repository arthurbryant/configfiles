Namespace('jp.mixi.featuredcontents.ui.nativesearchform')
.use('jp.co.mixi.ui.textareawithplaceholder clearTextarea')
.use('jp.mixi.featuredcontents.ui.searchform getInitializedForm')
.define(function (ns) {

    var createURL = function(action, parameters) {
       var url; 

       return url;
    };

    var createRequest = function(searchType) {

        var form =  ns.getInitializedForm(); 
        
        var searchText;
        var category_id;
        var parameters = {};
        var formElements = form.find("input[name]");
        formElements.each(
            function(index){
                var element = $j(this);
                paramters[element.attr("name")] = element.val();
            });
        var action = form.attr("action");        
        var url = createURL(parameters); 
    };
});
