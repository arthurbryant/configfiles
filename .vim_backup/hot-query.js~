function addHotQuery(element){
    // Add hot query to search box
    var bubble = $(element);
    var query = bubble.find("h5").text();
    console.dir(query);
    var input = $('#input');
    var inputValue = input.val();
    if(inputValue.length > 0){
        inputValue += " ";
    }
    input.val(inputValue + query);
    // Todo: make the bubble disapear
    bubble.remove();
}
