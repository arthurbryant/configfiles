#
# 日時のフォーマットはNove::Core::Datetime::datetime_to_epochで変換をかけるので
# 解釈できるフォーマットで記述してください(わからなければサンプルのに従ってください)
# deviceは配信対象としたいデバイスを選択してください
# 含まれないデバイスには配信されません
# 今のところ有効なデバイスは'android'と'iPhone'です
# 文字コードはutf-8を使用してください
#
# example
#
# ---
 device : [android]
 name : 'dummy_campaign_android1日本語'
 landingPage : 'http://url.to/android/campaign/1'
 image : 'http://url.to/android/campaign/image1'
 duration :
     startDate : 2013/01/16 19:20
     endDate : 2013/02/28 19:20
 isNewDuration :
     startDate : 2013/01/16 19:20
     endDate : 2013/02/02 19:23
# ---
# device : [iPhone]
# name : 'dummy_campaign_iPhone1'
# landingPage : 'http://url.to/iPhone/campaign/1'
# image : 'http://url.to/iPhone/campaign/image1'
# duration :
#     startDate : 2013/01/16 19:20
#     endDate : 2013/02/28 19:20
# isNewDuration :
#     startDate : 2013/01/16 19:20
#     endDate : 2013/02/02 19:23
# ---
# device : [android, iPhone]
# name : 'dummy_campaign_both1'
# landingPage : 'http://url.to/both/campaign/1'
# image : 'http://url.to/both/campaign/image1'
# duration :
#     startDate : 2013/01/16 19:20
#     endDate : 2013/02/28 19:20
# isNewDuration :
#     startDate : 2013/01/16 19:20
#     endDate : 2013/02/02 19:23

