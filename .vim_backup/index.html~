        hello, arthur!
        <p>
var http = require('http');
var fs = require('fs');
var port = '4000';
http.createServer(function(req, res) {
    fs.readFile('index.html', function(err, content) {
        res.writeHead(200, {'Content-Type':'text/html; charset=utf-8'} );
        res.end(content);
         });
}).listen( port );
console.log( 'Server running at port' + port );
</p>
<h1>Rebase head</h1>
<B>
    git commit -a</br> 
    git commit -m</br>
    <B>
        <p>git rebase test1</p>
        <p>git rebase test3</p>
        <p>git reset test</p>
        <p>git reset test</p>
