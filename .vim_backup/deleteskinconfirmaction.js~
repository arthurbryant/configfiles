Namespace('jp.mixi.skin.touch.admin.widget.deleteskinconfirmaction')
.define(function(ns) {
    /*global $j:false */
    "use strict";

    var confirmActions = function(element) {
        var submitButton = $j(element).find(".JS_submit");
        submitButton.on("click", function(){
            return window.confirm("本当に削除しますか");
        });
    };

    ns.provide({
        registerElement : confirmActions
    });
});
