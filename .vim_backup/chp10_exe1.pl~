package Oogaboogoo;

#use strict;
#use warnings;

use Data::Dumper;

@day = qw(ark dip wap sen pop sep kir);
@month = qw(diz pod bod rod sip wax lin sen kun fiz nap dep);

sub day_change {
	my $index = shift;
	return undef unless ($index =~ /[0-6]/);
	return $day[$index]; 
}
sub month_change {
	my $index = shift;
	return undef unless ($index =~ /[0-9]|10|11]/);
	return $month[$index];
}

chomp(my $input = <>);
my $result = &month_change($input);
print Dumper($result);
